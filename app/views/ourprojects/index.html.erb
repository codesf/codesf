<h1>Listing ourprojects</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Website</th>
    <th>Contributors</th>
    <th>Info</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ourprojects.each do |ourproject| %>
  <tr>
    <td><%= ourproject.name %></td>
    <td><%= ourproject.website %></td>
    <td><%= ourproject.contributors %></td>
    <td><%= ourproject.info %></td>
    <td><%= link_to 'Show', ourproject %></td>
    <td><%= link_to 'Edit', edit_ourproject_path(ourproject) %></td>
    <td><%= link_to 'Destroy', ourproject, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Ourproject', new_ourproject_path %>
