<h1>Listing cons</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Website</th>
    <th>Date</th>
    <th>Location</th>
    <th>Info</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cons.each do |con| %>
  <tr>
    <td><%= con.name %></td>
    <td><%= con.website %></td>
    <td><%= con.date %></td>
    <td><%= con.location %></td>
    <td><%= con.info %></td>
    <td><%= link_to 'Show', con %></td>
    <td><%= link_to 'Edit', edit_con_path(con) %></td>
    <td><%= link_to 'Destroy', con, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Con', new_con_path %>
